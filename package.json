{
  "name": "eleventy-with-vite",
  "version": "1.0.0",
  "description": "A demo or template for integrating a Vite build with Eleventy",
  "author": "Fotis Papadogeorgopoulos (fotis@fpapado.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/fpapado/eleventy-with-vite"
  },
  "bugs": {
    "url": "https://github.com/fpapado/eleventy-with-vite"
  },
  "main": "index.js",
  "dependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@babel/standalone": "7.13.14",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@vitejs/plugin-legacy": "^1.3.2",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "concurrently": "^6.0.0",
    "serve": "^11.3.2",
    "typescript": "^4.2.3",
    "vite": "^2.1.4"
  },
  "scripts": {
    "dev": "concurrently --raw npm:dev:*",
    "dev:tsc:watch": "tsc --watch --preserveWatchOutput",
    "dev:eleventy": "eleventy --serve",
    "dev:vite": "vite",
    "build": "npm run build:tsc && npm run build:vite && npm run build:eleventy",
    "build:tsc": "tsc",
    "build:eleventy": "eleventy",
    "build:vite": "vite build",
    "prod": "NODE_ENV=production npm run build && serve _site"
  }
}
