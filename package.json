{
  "name": "eleventy-with-vite",
  "version": "1.0.0",
  "description": "A demo or template for integrating a Vite build with Eleventy",
  "author": "Fotis Papadogeorgopoulos (fotis@fpapado.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/fpapado/eleventy-with-vite"
  },
  "bugs": {
    "url": "https://github.com/fpapado/eleventy-with-vite"
  },
  "main": "index.js",
  "dependencies": {},
  "devDependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@vitejs/plugin-legacy": "^1.3.2",
    "concurrently": "^6.0.0",
    "serve": "^11.3.2",
    "vite": "^2.1.3",
    "npm-run-all": "*",
    "cross-env": "*"
  },
  "scripts": {
    "dev": "concurrently --prefix-colors cyan,magenta --names vite,11ty --kill-others true npm:dev:a:vite npm:dev:b:eleventy",
    "dev:a:vite": "vite",
    "dev:b:eleventy": "eleventy --serve",
    "build": "npm-run-all -s build:**",
    "build:a:vite": "cross-env NODE_ENV=production vite build",
    "build:b:eleventy": "cross-env NODE_ENV=production eleventy",
    "prod": "npm-run-all -s prod:**",
    "prod:a:build": "cross-env NODE_ENV=production npm run build",
    "prod:b:serve": "serve _site",
    "comment:prod:b:serve": "echo serve _site: _site should be dynamic"
  }
}
